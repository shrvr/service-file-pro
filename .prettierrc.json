{
  "overrides": [
    {
      "files": ".gitignore",
      "options": {
        "parser": "ignore"
      }
    },
    {
      "files": ".husky/pre-commit",
      "options": {
        "parser": "json"
      }
    }
  ],
  "tabWidth": 2,
  "semi": true,
  "printWidth": 80,
  "singleQuote": false,
  "trailingComma": "all",
  "jsxSingleQuote": false,
  "bracketSpacing": true
}
